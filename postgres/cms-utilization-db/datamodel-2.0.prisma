model Provider {
  name = "providers"

  npi                                   int                                       @primary
  address_city                          string?                                   
  address_country                       string?                                   
  address_latitude                      float?                                    
  address_longitude                     float?                                    
  address_state                         string?                                   
  address_street01                      string?                                   
  address_street02                      string?                                   
  address_zip_code                      string?                                   
  entity_type                           string?                                   
  provider_performance                  ProviderPerformance(npi)[]                
  providers_individuals                 ProvidersIndividual(npi)[]                
  providers_organizations               ProvidersOrganization(npi)[]              
  provider_type                         string?                                   
  service_provider_performance          ServiceProviderPerformance(npi)[]         
  service_provider_performance_summary  ServiceProviderPerformanceSummary(npi)[]  
}

model ProviderPerformance {
  name = "provider_performance"

  prisma_id                                     int                  @primary @sequence(name: "provider_performance_prisma_id_seq1") @unique
  avg_mcare_allowed_amt                         float?               
  avg_mcare_pay_amt                             float?               
  avg_mcare_standardized_amt                    float?               
  avg_submitted_charge_amt                      float?               
  hcpcs_code                                    Service(prisma_id)   
  mcare_participation_indicator                 string?              
  n_of_distinct_mcare_beneficiary_per_day_svcs  int?                 
  n_of_mcare_beneficiaries                      int?                 
  n_of_svcs                                     float?               
  npi                                           Provider(prisma_id)  
  place_of_service                              string               
}

model ProvidersIndividual {
  name = "providers_individuals"

  npi          Provider(npi)  @primary
  credentials  string?        
  gender       string?        
  name_first   string?        
  name_last    string?        
  name_middle  string?        
}

model ProvidersOrganization {
  name = "providers_organizations"

  npi   Provider(npi)  @primary
  name  string?        
}

model Service {
  name = "services"

  hcpcs_code                    id                                        @primary
  hcpcs_description             string?                                   
  hcpcs_drug_indicator          string?                                   
  provider_performance          ProviderPerformance(hcpcs_code)[]         
  service_performance           ServicePerformance(hcpcs_code)[]          
  service_provider_performance  ServiceProviderPerformance(hcpcs_code)[]  
}

