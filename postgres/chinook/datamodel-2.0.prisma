model Album {
  name = "null"

  album_id   int                @primary
  artist_id  Artist(AlbumId)    
  title      string             
  track      Track(AlbumId)[]?  
}

model Artist {
  name = "null"

  artist_id  int                @primary
  album      Album(ArtistId)[]  
  name       string?            
}

model Customer {
  name = "null"

  customer_id     int                    @primary
  address         string?                
  city            string?                
  company         string?                
  country         string?                
  email           string                 
  fax             string?                
  first_name      string                 
  invoice         Invoice(CustomerId)[]  
  last_name       string                 
  phone           string?                
  postal_code     string?                
  state           string?                
  support_rep_id  Employee(CustomerId)?  
}

model Employee {
  name = "null"

  employee_id  int                      @primary
  address      string?                  
  birth_date   datetime?                
  city         string?                  
  country      string?                  
  customer     Customer(EmployeeId)[]?  
  email        string?                  
  employee     Employee(EmployeeId)[]?  
  fax          string?                  
  first_name   string                   
  hire_date    datetime?                
  last_name    string                   
  phone        string?                  
  postal_code  string?                  
  reports_to   Employee(EmployeeId)?    
  state        string?                  
  title        string?                  
}

model Genre {
  name = "null"

  genre_id  int                @primary
  name      string?            
  track     Track(GenreId)[]?  
}

model Invoice {
  name = "null"

  invoice_id           int                       @primary
  billing_address      string?                   
  billing_city         string?                   
  billing_country      string?                   
  billing_postal_code  string?                   
  billing_state        string?                   
  customer_id          Customer(InvoiceId)       
  invoice_date         datetime                  
  invoice_line         InvoiceLine(InvoiceId)[]  
  total                float                     
}

model InvoiceLine {
  name = "null"

  invoice_line_id  int                     @primary
  invoice_id       Invoice(InvoiceLineId)  
  quantity         int                     
  track_id         Track(InvoiceLineId)    
  unit_price       float                   
}

model MediaType {
  name = "null"

  media_type_id  int                   @primary
  name           string?               
  track          Track(MediaTypeId)[]  
}

model Playlist {
  name = "null"

  playlist_id     int                          @primary
  name            string?                      
  playlist_track  PlaylistTrack(PlaylistId)[]  
}

model PlaylistTrack {
  name = "null"

  playlist_id  Playlist()  
  track_id     Track()     
}

model Track {
  name = "null"

  track_id        int                       @primary
  album_id        Album(TrackId)?           
  bytes           int?                      
  composer        string?                   
  genre_id        Genre(TrackId)?           
  invoice_line    InvoiceLine(TrackId)[]    
  media_type_id   MediaType(TrackId)        
  milliseconds    int                       
  name            string                    
  playlist_track  PlaylistTrack(TrackId)[]  
  unit_price      float                     
}
