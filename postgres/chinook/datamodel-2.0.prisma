model Album {
  meta = {
    name = "null"
  }

  album_id   int       
  artist_id  Artist    
  title      string    
  track      Track[]?  
}

model Artist {
  meta = {
    name = "null"
  }

  artist_id  int       
  album      Album[]   
  name       string?   
}

model Customer {
  meta = {
    name = "null"
  }

  customer_id     int        
  address         string?    
  city            string?    
  company         string?    
  country         string?    
  email           string     
  fax             string?    
  first_name      string     
  invoice         Invoice[]  
  last_name       string     
  phone           string?    
  postal_code     string?    
  state           string?    
  support_rep_id  Employee?  
}

model Employee {
  meta = {
    name = "null"
  }

  employee_id     int          
  address         string?      
  birth_date      datetime?    
  city            string?      
  country         string?      
  customer        Customer[]?  
  email           string?      
  employee        Employee[]?  
  fax             string?      
  first_name      string       
  hire_date       datetime?    
  last_name       string       
  phone           string?      
  postal_code     string?      
  reports_to      Employee?    
  state           string?      
  title           string?      
}

model Genre {
  meta = {
    name = "null"
  }

  genre_id        int          
  name            string?      
  track           Track[]?     
}

model Invoice {
  meta = {
    name = "null"
  }

  invoice_id           int            
  billing_address      string?        
  billing_city         string?        
  billing_country      string?        
  billing_postal_code  string?        
  billing_state        string?        
  customer_id          Customer       
  invoice_date         datetime       
  invoice_line         InvoiceLine[]  
  total                float          
}

model InvoiceLine {
  meta = {
    name = "null"
  }

  invoice_line_id      int            
  invoice_id           Invoice        
  quantity             int            
  track_id             Track          
  unit_price           float          
}

model MediaType {
  meta = {
    name = "null"
  }

  media_type_id        int            
  name                 string?        
  track                Track[]        
}

model Playlist {
  meta = {
    name = "null"
  }

  playlist_id          int              
  name                 string?          
  playlist_track       PlaylistTrack[]  
}

model PlaylistTrack {
  meta = {
    name = "null"
  }

  playlist_id          Playlist         
  track_id             Track            
}

model Track {
  meta = {
    name = "null"
  }

  track_id             int              
  album_id             Album?           
  bytes                int?             
  composer             string?          
  genre_id             Genre?           
  invoice_line         InvoiceLine[]    
  media_type_id        MediaType        
  milliseconds         int              
  name                 string           
  playlist_track       PlaylistTrack[]  
  unit_price           float            
}
